from decimal import *

grade = float(0.0)
pregrade = 0.0
gender = "w"
height = 0
student_2 = input("")
pregrade = float(student_2[0:3])
gender = str(student_2[4])
height = int(student_2[6:9])

if height < 100 or height > 300:
    print("ERROR")
    raise SystemExit
else:
    pass
if gender == "w":
    grade = pregrade - 0.4
else:
